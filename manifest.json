{
  "manifest_version": 2,

  "name": "Tag",
  "description": "Chrome Extensions that Creates Bookmarks",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "topSites",
    "bookmarks"
  ], 
  "chrome_url_overrides" : {
    "newtab": "newTab.html"
  }
}
